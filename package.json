{
  "name": "truth-or-dare",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": ">=1.0.0 <2.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.11.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.11.0",
    "react-scripts": "3.0.1",
    "react-slick": "^0.29.0",
    "redux": "^4.2.1",
    "slick-carousel": "^1.8.1",
    "uuid": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "test-ci": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --single-quote --tab-width 4 --print-width 100 --write \"src/**/*.{jsx,js}\"",
    "lint": "eslint src/**/*.{jsx,js}",
    "lint-fix": "eslint --fix src/**/*.{jsx,js}"
  },
  "lint-staged": {
    "src/**/*.{jsx,js}": [
      "prettier --single-quote --tab-width 4 --print-width 100 --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-redtn-react": "^0.2.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "node-sass-chokidar": "^2.0.0",
    "prettier": "^2.8.7",
    "surge": "^0.23.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
